VITE_GOOGLE_MAPS_API_KEY=

# Optional: Dashboard data source
# If set, the Dashboard will POST `{ "endpoint": "dashboard/stats" }` (etc.) to this webhook.
# Use your n8n test or production URL in .env.local (do not commit it).
VITE_N8N_WEBHOOK_URL=

# Optional: Chat data source
# If set, YarnGPT chat will POST `{ "endpoint": "chat", "preferredLanguage": "en", "message": "..." }` to this webhook.
# If not set, it falls back to VITE_N8N_WEBHOOK_URL, then to mock responses.
VITE_N8N_CHAT_WEBHOOK_URL=

# Optional: Clinic-only Dashboard access code
# This is checked client-side (not secure). For real security, validate on a backend.
VITE_CLINIC_DASHBOARD_PASSWORD=

# Optional: Dashboard refresh interval (ms)
VITE_DASHBOARD_POLL_MS=10000
